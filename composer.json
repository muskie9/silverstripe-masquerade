{
    "name": "dhensby/silverstripe-masquerade",
    "description": "SilverStripe module to allow you to masquerade as other users",
    "type": "silverstripe-module",
    "require": {
        "php": "^5.3.3 || ^7",
        "silverstripe/framework": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^3 || ^4 || ^5"
    },
    "license": "ISC",
    "authors": [
        {
            "name": "Dan Hensby",
            "email": "github@dhensby.co.uk"
        }
    ],
    "extra": {
        "branch-alias": {
            "master-dev": "2.x-dev"
        },
        "installer-name": "masquerade"
    },
    "autoload": {
        "psr-4": {
            "DHensby\\Masquerade\\": "src/",
            "DHensby\\Masquerade\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
